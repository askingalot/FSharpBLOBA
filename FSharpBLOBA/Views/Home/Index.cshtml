@model System.Collections.Generic.IEnumerable<Model.TroubleTicket>

<div class="jumbotron">
    <h1>F# BLOBA</h1>
    <h2>Trouble Tickets</h2>
</div>

<div class="panel">
    <div class="panel-heading">Unresolved Tickets</div>
    <div class="panel-body">
        @foreach (var ticket in Model)
        {
            <div class="row">
                <h3 class="col-xs-8">@ticket.Summary</h3>
                <h3 class="col-xs-4">@ticket.OccurrenceDateTime</h3>
            </div>
            <div class="row">
                <div class="col-xs-8">@ticket.Details</div>
            </div>
        }
    </div>
</div>